listen: 127.0.0.1:4222
jetstream {
    enable: true
    # JetStream storage location
    store_dir: "./nats.db/"
    # 1GB
    max_memory_store: 1073741824
    # 10GB
    max_file_store: 10737418240
    sync_interval: always
}
max_payload: 8MB
server_name: "enclave_bus_nats_server"
trace_verbose: true
debug: true
logtime: true
log_file: "./.logs/nats.log"
logfile_size_limit: 1GB
logfile_max_num: 10
http: 127.0.0.1:4242
http_base_path: "/mon/"
